{
    "name": "hechoenlaravel/jarvis-foundation",
    "description": "Some foundation code for the Jarvis Platform",
    "license": "MIT",    
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hechoenlaravel/composer-plugin"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "pingpong/sky": "~2.1",
        "pingpong/modules": "~2.1",
        "pingpong/menus": "~2.1",
        "pingpong/widget" : "~2.1",
        "pingpong/generators": "~2.1",
        "barryvdh/laravel-debugbar": "^2.1@dev",
        "styde/html": "~1.0",
        "joselfonseca/laravel-api-tools": "1.3.*",
        "joselfonseca/laravel-tactician" : "0.2.*",
        "joselfonseca/image-manager" : "2.1.*",
        "laracasts/utilities": "~2.0",
        "uxweb/sweet-alert": "~1.1",
        "yajra/laravel-datatables-oracle": "~5.0",
        "baum/baum": "~1.1",
        "cviebrock/eloquent-sluggable" : "3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.0",
        "orchestra/testbench": "~3.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Hechoenlaravel\\JarvisFoundation\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Hechoenlaravel\\JarvisFoundation\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "preferred-install": "dist"
    }
}
