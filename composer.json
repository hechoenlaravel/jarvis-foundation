{
    "name": "hechoenlaravel/jarvis-foundation",
    "description": "Some foundation code for the Jarvis Platform",
    "license": "MIT",    
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hechoenlaravel/composer-plugin"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.4.*",
        "hechoenlaravel/jarvis-ui" : "2.0.*",
        "nwidart/laravel-modules": "1.16.*",
        "barryvdh/laravel-debugbar": "^2.3",
        "styde/html": "~1.3",
        "joselfonseca/laravel-api-tools": "2.2.*",
        "joselfonseca/laravel-tactician" : "0.3.*",
        "laracasts/utilities": "~2.1",
        "uxweb/sweet-alert": "~1.4",
        "yajra/laravel-datatables-oracle": "~7.0",
        "maatwebsite/excel": "~2.1",
        "venturecraft/revisionable": "1.*",
        "webpatser/laravel-uuid": "2.*",
        "doctrine/dbal": "2.5.*",
        "predis/predis": "^1.1",
        "spatie/laravel-permission": "^1.7",
        "intervention/image": "^2.3",
        "yajra/laravel-datatables-html": "^1.0",
        "hechoenlaravel/jarvis-menus": "2.3.*",
        "sentry/sentry-laravel": "^0.6.0",
        "spatie/laravel-backup" : "^3.0",
        "maknz/slack": "^1.7",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7",
        "orchestra/testbench": "3.4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Hechoenlaravel\\JarvisFoundation\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Hechoenlaravel\\JarvisFoundation\\Tests\\": "tests/"
      }
    },
    "config": {
      "preferred-install": "dist"
    }
}
