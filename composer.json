{
    "name": "hechoenlaravel/jarvis-foundation",
    "description": "Some foundation code for the Jarvis Platform",
    "license": "MIT",    
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "barryvdh/laravel-debugbar": "^2.3",
        "doctrine/dbal": "2.5.*",
        "hechoenlaravel/jarvis-ui": "2.0.*",
        "hechoenlaravel/jarvis-menus": "2.4.*",
        "intervention/image": "^2.3",
        "joselfonseca/laravel-tactician": "0.3.*",
        "laracasts/utilities": "~2.1",
        "laravel/framework": "5.5.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "~2.1",
        "maknz/slack": "^1.7",
        "nwidart/laravel-modules": "^2.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^0.6.0",
        "spatie/laravel-backup": "^3.0",
        "spatie/laravel-fractal": "^5.2",
        "spatie/laravel-permission": "^2.7",
        "styde/html": "~1.4",
        "uxweb/sweet-alert": "~1.4",
        "venturecraft/revisionable": "1.*",
        "webpatser/laravel-uuid": "2.*",
        "yajra/laravel-datatables-html": "^3.2",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*",
        "mockery/mockery": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Hechoenlaravel\\JarvisFoundation\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Hechoenlaravel\\JarvisFoundation\\Tests\\": "tests/"
      }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
